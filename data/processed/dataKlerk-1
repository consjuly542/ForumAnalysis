442 \u0435\u0449\u0435 \u0434\u043e\u043a\u0430\u0437\u0430\u0442\u0435\u043b\u044c\u0441\u0442\u0432 \u0438 \u043a\u043e\u043d\u0435\u0447\u043d\u0443\u044e \u0442\u043e\u0447\u043a\u0443 \u043f\u043e\u0441\u0442\u0430\u0432\u0438\u0442 \u0442\u043e\u043b\u044c\u043a\u043e \u0441\u0443\u0434.\n\n\n\u0415\u0441\u043b\u0438 \u0412\u044b \u043f\u0440\u043e\u0441\u0442\u043e \u043f\u0435\u0440\u0435\u043f\u0440\u043e\u0434\u0430\u0432\u0435\u0446 \u0440\u0430\u0431\u043e\u0442 (\u0443\u0441\u043b\u0443\u0433), \u0442\u043e \u0431\u0443\u0434\u0443\u0442 \u043f\u0440\u043e\u0432\u0435\u0440\u044f\u0442\u044c \u0438\u043d\u0444\u043e\u0440\u043c\u0430\u0446\u0438\u044e \u0443\u0436\u0435 \u0412\u0430\u0448\u0438\u0445 \u043f\u043e\u0441\u0442\u0430\u0432\u0449\u0438\u043a\u043e\u0432 -\u0437\u0430\u043f\u0440\u0430\u0448\u0438\u0432\u0430\u0442\u044c \u0434\u043e\u043a\u0443\u043c\u0435\u043d\u0442\u044b \u0438 \u0430\u043d\u0430\u043b\u0438\u0437\u0438\u0440\u043e\u0432\u0430\u0442\u044c \u0438\u0445. \u0415\u0441\u043b\u0438 \u044d\u0442\u043e \u0440\u0430\u0431\u043e\u0442\u044b, \u0442\u0440\u0435\u0431\u0443\u044e\u0449\u0438\u0435 \u0434\u043e\u043f\u0443\u0441\u043a \u0421\u0420\u041e, \u0442\u043e \u0433\u0440\u0430\u043c\u043e\u0442\u043d\u044b\u0439 \u0432\u044b\u0435\u0437\u0434\u043d\u0438\u043a \u044d\u0442\u043e \u043e\u0431\u044f\u0437\u0430\u0442\u0435\u043b\u044c\u043d\u043e \u043f\u0440\u043e\u0432\u0435\u0440\u0438\u0442 \u0438 \u0437\u0430\u043f\u0440\u043e\u0441\u0438\u0442 \u0432 \u0421\u0420\u041e \u0434\u043e\u043a\u0443\u043c\u0435\u043d\u0442\u044b. \u041f\u043b\u044e\u0441 \u043c\u043e\u0433\u0443\u0442 \u043f\u0440\u043e\u0432\u0435\u0441\u0442\u0438 \u044d\u043a\u0441\u043f\u0435\u0440\u0442\u0438\u0437\u0443 \u043f\u043e \u0440\u0430\u0431\u043e\u0442\u0430\u043c. \u041d\u043e \u0442\u0430\u043a \u043a\u0430\u043a \u044d\u0442\u043e \u043d\u0435 \u0434\u0451\u0448\u0435\u0432\u043e\u0435 \u0443\u0434\u043e\u0432\u043e\u043b\u044c\u0441\u0442\u0432\u0438\u0435, \u0442\u043e \u0432\u0441\u0435 \u0431\u0443\u0434\u0435\u0442 \u0437\u0430\u0432\u0438\u0441\u0435\u0442\u044c \u043e\u0442 \u0441\u0443\u043c\u043c\u044b \"\u043d\u0430 \u043a\u043e\u043d\u0443\".\n\n\n\u0415\u0441\u043b\u0438 \u0412\u044b \u0435\u0449\u0435 \u0438 \u043f\u0435\u0440\u0435\u043f\u0440\u043e\u0434\u0430\u0432\u0430\u043b\u0438 \u0442\u043e\u0432\u0430\u0440\u044b, \u0438\u0445 \"\u0438\u0441\u0442\u043e\u0440\u0438\u044e\" \u0442\u043e\u0436\u0435 \u043f\u0440\u043e\u0432\u0435\u0440\u044f\u0442.\n\n\n\u0427\u0442\u043e \u043a\u0430\u0441\u0430\u0435\u0442\u0441\u044f \u043e\u0431\u043e\u0440\u043e\u0442\u043e\u043a.\n\n\u041e\u043f\u044f\u0442\u044c \u0436\u0435 \u0431\u0443\u0434\u0443\u0442 \u0438\u0441\u043a\u0430\u0442\u044c \u043d\u0435\u0441\u0442\u044b\u043a\u043e\u0432\u043a\u0438 \u0438 \u0434\u043e\u043f\u043e\u043b\u043d\u0438\u0442\u0435\u043b\u044c\u043d\u044b\u0435 \u0441\u0432\u0435\u0434\u0435\u043d\u0438\u044f.\n\n\u0412\u0435\u0434\u044c \u043f\u043e \u043e\u0431\u043e\u0440\u043e\u0442\u043a\u0430\u043c \u043c\u043e\u0436\u043d\u043e \u0443\u0432\u0438\u0434\u0435\u0442\u044c \u0434\u0432\u0438\u0436\u0435\u043d\u0438\u0435 \u0434\u0435\u043d\u0435\u0433, \u0442\u043e\u0432\u0430\u0440\u043e\u0432 \u0438 \u0443\u0441\u043b\u0443\u0433. \u0418 \u043a\u043e\u043d\u0435\u0447\u043d\u043e\u0435 \u0441\u0430\u043b\u044c\u0434\u043e. \n\n\u0411\u044b\u0432\u0430\u0435\u0442 \u0442\u0430\u043a, \u0447\u0442\u043e \u0430\u043d\u0430\u043b\u0438\u0442\u0438\u043a\u0430 (\u0432\u0432\u0435\u0434\u0435\u043d\u043d\u0430\u044f \u0412\u0430\u043c\u0438) \u0432 \u0431\u0443\u0445\u0433\u0430\u043b\u0442\u0435\u0440\u0441\u043a\u043e\u0439 \u043f\u0440\u043e\u0433\u0440\u0430\u043c\u043c\u0435 \u043f\u043e \u0434\u043e\u0433\u043e\u0432\u043e\u0440\u0430\u043c \u0438 \u0440\u0430\u0431\u043e\u0442\u0430\u043c \u043e\u0442\u043b\u0438\u0447\u0430\u0435\u0442\u0441\u044f \u043f\u043e \u043d\u0430\u0438\u043c\u0435\u043d\u043e\u0432\u0430\u043d\u0438\u044e \u043e\u0442 \u0434\u043e\u043a\u0443\u043c\u0435\u043d\u0442\u043e\u0432, \u043a\u043e\u0442\u043e\u0440\u044b\u0435 \u0435\u0441\u0442\u044c \u0443 \u043f\u043e\u043a\u0443\u043f\u0430\u0442\u0435\u043b\u044f (\u0441\u043e\u043e\u0442\u0432\u0435\u0442\u0441\u0442\u0432\u0435\u043d\u043d\u043e \u0438 \u0443\u0436\u0435 \u0443 \u043f\u0440\u043e\u0432\u0435\u0440\u043a\u0438) \u0438 \u044d\u0442\u043e \u0442\u043e\u0436\u0435 \u043f\u0440\u0438\u0432\u043b\u0435\u043a\u0430\u0435\u0442 \u0432\u043d\u0438\u043c\u0430\u043d\u0438\u0435. \u0411\u0443\u0434\u0435\u0442 \u044d\u0442\u043e \u043a\u0430\u043a-\u0442\u043e \u0438\u0441\u043f\u043e\u043b\u044c\u0437\u043e\u0432\u0430\u043d\u043e \u0432 \u0442\u0435\u043a\u0441\u0442\u0435 \u0430\u043a\u0442\u0430 \u0432\u043d\u043f \u0438\u043b\u0438 \u043d\u0435\u0442, \u044d\u0442\u043e \u0443\u0436\u0435 \u0440\u0435\u0448\u0438\u0442 \u0438\u043d\u0441\u043f\u0435\u043a\u0442\u043e\u0440 \u0438 \u0435\u0433\u043e \u043d\u0430\u0447\u0430\u043b\u044c\u043d\u0438\u043a, \u0441\u043e\u0433\u043b\u0430\u0441\u043e\u0432\u044b\u0432\u0430\u044e\u0449\u0438\u0439 \u0430\u043a\u0442, \u0432 \u0437\u0430\u0432\u0438\u0441\u0438\u043c\u043e\u0441\u0442\u0438 \u043e\u0442 \u0432\u043e\u0437\u043c\u043e\u0436\u043d\u043e\u0439 \u043f\u043e\u043b\u044c\u0437\u044b \u0432\u044b\u0432\u043e\u0434\u043e\u0432 \u043e\u0431 \u044d\u0442\u043e\u043c.\n\n\n\u0421\u0447.90, 91 -\u043f\u043e \u0441\u0443\u0442\u0438 \u044d\u0442\u043e \u043f\u0440\u043e\u0432\u0435\u0440\u043a\u0430 \u0412\u0430\u0441, \u0432\u0435\u0440\u043d\u0435\u0435 \u0442\u043e\u0433\u043e \u043a\u0430\u043a \u0412\u044b \u043e\u0442\u0440\u0430\u0437\u0438\u043b\u0438 \u0438 \u043e\u0442\u0440\u0430\u0437\u0438\u043b\u0438 \u043b\u0438 \u0432\u043e\u043e\u0431\u0449\u0435 \u0440\u0435\u0430\u043b\u0438\u0437\u0430\u0446\u0438\u044e \u0432 \u0441\u0432\u043e\u0435\u043c \u0443\u0447\u0435\u0442\u0435.\n\n\u041d\u0435 \u0432\u0441\u0435\u0433\u0434\u0430 \u0432\u0441\u0435 \u0437\u0430\u043f\u0440\u043e\u0448\u0435\u043d\u043d\u044b\u0435 \u0441\u0447\u0435\u0442\u0430 \u044d\u0442\u043e \u043d\u0435\u043e\u0431\u0445\u043e\u0434\u0438\u043c\u043e\u0441\u0442\u044c \u0438\u043d\u0441\u043f\u0435\u043a\u0442\u043e\u0440\u0430, \u0438\u043d\u043e\u0433\u0434\u0430 \u044d\u0442\u043e \u0440\u0430\u0431\u043e\u0442\u0430 \u043f\u043e\u0434 \u0448\u0430\u0431\u043b\u043e\u043d \u0442\u043e\u0433\u043e, \u0447\u0442\u043e \u0434\u043e\u043b\u0436\u043d\u043e \u0431\u044b\u0442\u044c \u0437\u0430\u043f\u0440\u043e\u0448\u0435\u043d\u043e.\n\n\n\u0414\u0430\u0436\u0435 \u0435\u0441\u043b\u0438 \u0412\u044b \u043f\u0440\u0435\u0434\u0441\u0442\u0430\u0432\u043b\u044f\u0435\u0442\u0435 \u043e\u0431\u043e\u0440\u043e\u0442\u043a\u0438, \u043f\u0440\u0435\u0434\u043e\u0441\u0442\u0430\u0432\u043b\u044f\u0439\u0442\u0435 \u0442\u043e\u043b\u044c\u043a\u043e \u043f\u043e \u044d\u0442\u043e\u043c\u0443 \u043f\u043e\u043a\u0443\u043f\u0430\u0442\u0435\u043b\u044e. \u041d\u0435 \u043d\u0443\u0436\u043d\u043e \u0434\u0430\u0432\u0430\u0442\u044c \u043b\u0438\u0448\u043d\u0435\u0439 \u0438\u043d\u0444\u043e\u0440\u043c\u0430\u0446\u0438\u0438, \u043a\u043e\u0442\u043e\u0440\u0430\u044f \u043a\u043e\u0433\u0434\u0430-\u043d\u0438\u0431\u0443\u0434\u044c \u043c\u043e\u0436\u0435\u0442 \u0441\u044b\u0433\u0440\u0430\u0442\u044c \u043f\u0440\u043e\u0442\u0438\u0432 \u0412\u0430\u0441, \u0442.\u043a. \u0412\u0430\u0448\u0430 \u043d\u0430\u043b\u043e\u0433\u043e\u0432\u0430\u044f \u043c\u043e\u0436\u0435\u0442 \u0438\u0441\u043f\u043e\u043b\u044c\u0437\u043e\u0432\u0430\u0442\u044c.\n\n\u0423\u0434\u0438\u0432\u0438\u0442\u0435\u043b\u044c\u043d\u043e, \u0447\u0442\u043e \u0443 \u0412\u0430\u0441 \u0441\u0447.10 \u043d\u0435 \u0437\u0430\u043f\u0440\u043e\u0441\u0438\u043b\u0438, \u043e\u0431\u044b\u0447\u043d\u043e \u044d\u0442\u043e \u0441\u0430\u043c\u044b\u0439 \u043e\u0431\u044a\u0435\u043c\u043d\u044b\u0439 \u0441\u0