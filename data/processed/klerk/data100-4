u0442\u044c \u043f\u0440\u043e\u0442\u0438\u0432 \u0412\u0430\u0441, \u0442.\u043a. \u0412\u0430\u0448\u0430 \u043d\u0430\u043b\u043e\u0433\u043e\u0432\u0430\u044f \u043c\u043e\u0436\u0435\u0442 \u0438\u0441\u043f\u043e\u043b\u044c\u0437\u043e\u0432\u0430\u0442\u044c.\n\n\u0423\u0434\u0438\u0432\u0438\u0442\u0435\u043b\u044c\u043d\u043e, \u0447\u0442\u043e \u0443 \u0412\u0430\u0441 \u0441\u0447.10 \u043d\u0435 \u0437\u0430\u043f\u0440\u043e\u0441\u0438\u043b\u0438, \u043e\u0431\u044b\u0447\u043d\u043e \u044d\u0442\u043e \u0441\u0430\u043c\u044b\u0439 \u043e\u0431\u044a\u0435\u043c\u043d\u044b\u0439 \u0441\u0447\u0435\u0442. \u0410\u043d\u0430\u043b\u0438\u0442\u0438\u043a\u0443 \u043f\u043e \u043a\u043e\u043d\u0442\u0440\u0430\u0433\u0435\u043d\u0442\u0443 \u043f\u043e \u043d\u0435\u043c\u0443 \u0441\u0434\u0435\u043b\u0430\u0442\u044c \u043d\u0435\u0432\u043e\u0437\u043c\u043e\u0436\u043d\u043e. \u0415\u0441\u043b\u0438 \u0437\u0430\u043f\u0440\u043e\u0441\u044f\u0442 \u043c\u043e\u0436\u043d\u043e \u0438\u0441\u043f\u043e\u043b\u044c\u0437\u043e\u0432\u0430\u0442\u044c \u043e\u0431\u043e\u0440\u043e\u0442\u043a\u0443 \u0437\u0430\u0442\u0440\u0430\u0442\u043d\u043e\u0433\u043e \u0441\u0447\u0435\u0442\u0430, \u043d\u0430 \u043a\u043e\u0442\u043e\u0440\u044b\u0439 \u0441\u043f\u0438\u0441\u044b\u0432\u0430\u043b\u0438 \u043c\u0430\u0442\u0435\u0440\u0438\u0430\u043b\u044b \u043f\u0440\u0438 \u0432\u044b\u043f\u043e\u043b\u043d\u0435\u043d\u0438\u0438 \u0440\u0430\u0431\u043e\u0442.\n\n\n\u041d\u0430\u0441\u0447\u0435\u0442 \u043e\u0442\u043a\u0443\u0434\u0430 \u043f\u0440\u043e\u0432\u0435\u0440\u043a\u0430 \u0443\u0437\u043d\u0430\u043b\u0430 \u043f\u0440\u043e \u0420\u043e\u043c\u0430\u0448\u043a\u0443 \u0438 \u0412\u0430\u0441\u0438\u043b\u044c\u043a\u0430. \n\n\u0412 \u043d\u0430\u043b\u043e\u0433\u043e\u0432\u044b\u0445 \u043f\u0435\u0440\u0435\u0434 \u0432\u044b\u0445\u043e\u0434\u043e\u043c \u043d\u0430 \u0412\u041d\u041f \u043f\u043b\u0430\u0442\u0435\u043b\u044c\u0449\u0438\u043a\u0430 \u043f\u0440\u043e\u0432\u043e\u0434\u044f\u0442 \u0430\u043d\u0430\u043b\u0438\u0437 \u0435\u0433\u043e \u0434\u0435\u044f\u0442\u0435\u043b\u044c\u043d\u043e\u0441\u0442\u0438: \u0430\u043d\u0430\u043b\u0438\u0437\u0438\u0440\u0443\u044e\u0442 \u0440\u0430\u0441\u0447\u0435\u0442\u043d\u044b\u0435 \u0441\u0447\u0435\u0442\u0430, \u0432\u044b\u044f\u0432\u043b\u044f\u044e\u0442 \u043a\u043e\u043d\u0442\u0440\u0430\u0433\u0435\u043d\u0442\u043e\u0432 \u0438 \u0443\u0436\u0435 \u0440\u0430\u0431\u043e\u0442\u0430\u044e\u0442 \u0441 \u043d\u0438\u043c\u0438. \u0417\u0430\u043f\u0440\u0430\u0448\u0438\u0432\u0430\u044e\u0442 \u0434\u043e\u043a\u0443\u043c\u0435\u043d\u0442\u044b. \u0427\u0435\u0440\u0435\u0437 \u0431\u0430\u0437\u0443 \u041d\u0414\u0424\u041b \u0432\u044b\u044f\u0432\u043b\u044f\u044e\u0442 \u0440\u0430\u0431\u043e\u0442\u043d\u0438\u043a\u043e\u0432 \u0432\u043e\u0437\u043c\u043e\u0436\u043d\u043e\u0433\u043e \u043f\u0440\u043e\u0432\u0435\u0440\u044f\u0435\u043c\u043e\u0433\u043e \u043b\u0438\u0446\u0430 \u0438 \u043f\u0440\u043e\u0432\u043e\u0434\u044f\u0442 \u0434\u043e\u043f\u0440\u043e\u0441\u044b \u0441\u0432\u0438\u0434\u0435\u0442\u0435\u043b\u0435\u0439. \u0422\u043e\u0436\u0435 \u0441\u0430\u043c\u043e\u0435 \u0441 \u043a\u043e\u043d\u0442\u0440\u0430\u0433\u0435\u043d\u0442\u0430\u043c\u0438 \u0438 \u0412\u0430\u0448\u0438\u043c\u0438 \u0432 \u0442.\u0447. \u0412\u043e\u0437\u043c\u043e\u0436\u043d\u043e \u0433\u0434\u0435-\u0442\u043e \u043f\u0440\u043e\u0441\u043a\u043e\u0447\u0438\u043b\u0430 \u0438\u043d\u0444\u043e\u0440\u043c\u0430\u0446\u0438\u044f \u0438\u043b\u0438 \u0436\u0435 \u043d\u0430\u043b\u043e\u0433\u043e\u0432\u0430\u044f \u043f\u0440\u0435\u0434\u043f\u043e\u043b\u043e\u0436\u0438\u043b\u0430, \u0438\u0437\u0443\u0447\u0438\u0432 \u0412\u0430\u0448 \u0440\u0430\u0441\u0447\u0435\u0442\u043d\u044b\u0439 \u0441\u0447\u0435\u0442.\n\n\n\u0421\u043f\u0430\u0441\u0438\u0431\u043e \u0412\u0430\u0