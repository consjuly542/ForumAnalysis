u0443 \u043e\u0442\u0447\u0435\u0442\u043d\u043e\u0441\u0442\u0438 \u0438 \u043f\u0440\u043e\u0432\u043e\u0434\u043e\u043a. \u041f\u0440\u0430\u0432\u0438\u043b\u044c\u043d\u043e \u043b\u0438 \u043f\u043e\u043d\u044f\u043b\u0430:\n\n\n\u0410\u041d\u041e \u043d\u0430 \u041e\u0421\u041d\u041e, \u0434\u0435\u044f\u0442\u0435\u043b\u044c\u043d\u043e\u0441\u0442\u0438 \u043d\u0435\u0442, \u0435\u0441\u0442\u044c \u0432\u0437\u043d\u043e\u0441\u044b \u043d\u0430 \u0423\u0414 \u0438  \u0435\u0441\u0442\u044c \u0440\u0430\u0441\u0445\u043e\u0434\u044b \u043d\u0430 \u0431\u0430\u043d\u043a (\u0420\u041a\u041e)\n\n1.\u041f\u0440\u043e\u0432\u043e\u0434\u043a\u0438 \u043f\u043e \u043f\u043e\u0441\u0442\u0443\u043f\u043b\u0435\u043d\u0438\u044e \u0434\u0435\u043d\u0435\u0436\u043d\u044b\u0445 \u0441\u0440\u0435\u0434\u0441\u0442\u0432 \u043d\u0430 \u0423\u0441\u0442\u0430\u0432\u043d\u0443\u044e \u0434\u0435\u044f\u0442\u0435\u043b\u044c\u043d\u043e\u0441\u0442\u044c \u0434\u0435\u043b\u0430\u044e \u0414 50 \u041a 76 (\u0432 \u0440\u0430\u0437\u0440\u0435\u0437\u0435 \u043f\u043e \u043a\u043e\u043d\u0442\u0440\u0430\u0433\u0435\u043d\u0442\u0430\u043c \u0438 \u0441\u0443\u043c\u043c\u0430\u043c)  \n\n\u0420\u0443\u0447\u043d\u0430\u044f \u043e\u043f\u0435\u0440\u0430\u0446\u0438\u044f \u0414 76 \u041a 86 \n\n\u0412\u0430\u0440\u0438\u0430\u043d\u0442 \u041451 \u041a86 \u043f\u0440\u043e\u0441\u0442\u043e\u0439 \u0438 \u0443\u0434\u043e\u0431\u043d\u044b\u0439 - \u043d\u043e \u044d\u0442\u043e \u0434\u043b\u044f \u0423\u0421\u041d, \u0432\u0435\u0440\u043d\u043e?\n\n\n2. \u0420\u041a\u041e \u043f\u0440\u043e\u0432\u043e\u0436\u0443 \u043d\u0430 91.02 \u0438 \u043f\u043e\u0442\u043e\u043c \u0440\u0443\u0447\u043d\u043e\u0439 \u043f\u0440\u043e\u0432\u043e\u0434\u043a\u043e\u0439 \u041486 \u041a 9102\n\n\n\u041d\u043e \u043d\u0430\u0448\u043b\u0430, \u0447\u0442\u043e \u044d\u0442\u043e \u043d\u0435\u0432\u0435\u0440\u043d\u043e,  \u0442\u0430\u043a \u043a\u0430\u043a \u043d\u0435\u0442 \u043f\u0440\u0435\u0434\u043f\u0440\u0438\u043d\u0438\u043c\u0430\u0442\u0435\u043b\u044c\u0441\u043a\u043e\u0439 \u0434\u0435\u044f\u0442\u0435\u043b\u044c\u043d\u043e\u0441\u0442\u0438 \u0438 \u043d\u0443\u0436\u043d\u043e \u043f\u0440\u043e\u0432\u043e\u0434\u0438\u0442\u044c \u043d\u0430 76.\n\n \u0422.\u0435. \u0420\u041a\u041e:  \u041476 \u041a 51 - \u0443\u0441\u043b\u0443\u0433\u0438 \u0431\u0430\u043d\u043a\u0430, \u0438 \u041486 \u041a76 - \u0437\u0430\u043a\u0440\u044b\u0442\u044c \u0443\u0441\u043b\u0443\u0433\u0438 \u0431\u0430\u043d\u043a\u0430. \u041f\u0440\u0430\u0432\u0438\u043b\u044c\u043d\u043e \u043b\u0438 \u044d\u0442\u043e \u0438\u043b\u0438 \u0440\u0430\u0437\u043d\u0438\u0446\u044b \u043c\u0435\u0436\u0434\u0443 \u043f\u0435\u0440\u0432\u044b\u043c \u0438 \u0432\u0442\u043e\u0440\u044b\u043c \u0432\u0430\u0440\u0438\u0430\u043d\u0442\u043e\u043c \u043d\u0435\u0442?\n\n\n3.\u0411\u0430\u043b\u0430\u043d\u0441 \u0431\u0443\u0434\u0435\u0442 \u0443\u043f\u0440\u043e\u0449\u0435\u043d\u043d\u044b\u0439 - \u0437\u0430\u043f\u043e\u043b\u043d\u044f\u044e\u0442\u0441\u044f \u0441\u0442\u0440\u043e\u043a\u0438 1250 (\u0414\u0435\u043d \u0441\u0440\u0435\u0434\u0441\u0442\u0432\u0430) \u0438 1350 (\u0426\u0435\u043b\u0435\u0432\u044b\u0435), \u041e\u0442\u0447\u0435\u0442 \u043e \u0446\u0435\u043b\u0435\u0432\u043e\u043c \u0438\u0441\u043f\u043e\u043b\u044c\u0437\u043e\u0432\u0430\u043d\u0438\u0438 - \u0432\u0440\u0443\u0447\u043d\u0443\u044e. (1\u0421 \u043d\u0435 \u043d\u0430\u0441\u0442\u0440\u043e\u0435\u043d\u0430 \u043f\u043e\u0434 \u041d\u041a\u041e). \u0422\u0430\u043a \u043c\u0430\u043b\u043e \u0437\u0430\u043f\u043e\u043b\u043d\u044f\u0442\u044c, \u0447\u0442\u043e \u043a\u0430\u0436\u0435\u0442\u0441\u044f, \u0447\u0442\u043e \u043d\u0435\u0432\u0435\u0440\u043d\u043e. \u0418 \u0432\u043e\u043f\u0440\u043e\u0441\u044b \u043f\u043e\u0442\u043e\u043c\u0443, \u043f\u0440\u043e\u0441\u0442\u0438\u0442\u0435, \u0433\u043b\u0443\u043f\u04